RewriteEngine On

RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)$ index.php?request=$1/$2 [L]

# Redirigir las vistas sin mostrar "views" en la URL
RewriteRule ^citas/index$ ../views/citas/index.php [L]
RewriteRule ^paciente/index$ ../views/home/index.php [L]
# Agrega más reglas según sea necesario para otras vistas
